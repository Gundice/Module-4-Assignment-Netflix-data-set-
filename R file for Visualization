# Load required library
library(png)

# Read the saved image file
img <- readPNG("C:\\Users\\Adeyemo Bolanle\\Desktop\\python codes\\most_watched_genres.png")

# Create an empty plot area with appropriate dimensions
plot(1:2, type = "n", xlab = "", ylab = "", xlim = c(0, 1), ylim = c(0, 1))

# Display the image with correct dimensions
rasterImage(img, 0, 0, 1, 1, interpolate = FALSE)
